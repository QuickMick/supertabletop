{
  "name": "tabletop",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "watch-js": "cmd /k watchify public/javascripts/main.js -o public/js/game.js -v -d",
    "editor-watch-js": "cmd /k watchify editor/editormain.js -o public/js/editor.js -v -d",
    "compile names":"node compilenames.js source=random_names.txt target=core/random_names.json",
    "test": "mocha",
    "debug": "node --inspect --debug-brk ./bin/www"
  },
  "devDependencies": {
    "mocha": "*",
    "watchify": "*"
  },
  "dependencies": {
    "bcrypt": "^1.0.2",
    "body-parser": "~1.16.0",
    "cookie-parser": "~1.4.3",
    "dateformat": "^2.0.0",
    "debug": "~2.6.0",
    "eventemitter3": "^2.0.3",
    "express": "~4.14.1",
    "glslify": "^6.0.2",
    "matter-js": "^0.12.0",
    "morgan": "~1.7.0",
    "pixi-display": "*",
    "pixi-extra-filters": "*",
    "pixi-filters": "^1.0.6",
    "pixi.js": "*",
    "pug": "^2.0.0-rc.2",
    "serve-favicon": "~2.3.2",
    "socket.io": "*",
    "socket.io-client": "*",
    "uuid": "^3.0.1"
  }
